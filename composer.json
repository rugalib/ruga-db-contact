{
  "name": "rugalib/ruga-db-contact",
  "description": "Contact entities for organizations and persons",
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/rugalib",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2",
    "psr/container": "^1.0",
    "laminas/laminas-form": "^3.4",
    "rugalib/ruga-db": "^1.3",
    "rugalib/ruga-log": "^1.0",
    "rugalib/ruga-db-person": "^1.0",
    "rugalib/ruga-db-organization": "^1.0",
    "rugalib/ruga-db-party": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "laminas/laminas-config-aggregator": "^1.9",
    "laminas/laminas-servicemanager": "^3.17",
    "rugalib/ruga-db-user": "^1.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.rugalib.ch/"
    }
  ],
  "authors": [
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\Contact\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ruga\\Contact\\Test\\": "test/src"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Contact",
      "config-provider": "Ruga\\Contact\\ConfigProvider"
    }
  }
}